{
    "id": "task-id-full",
    "state": "COMPLETE",
    "name": "test-task-full",
    "description": "A full TES task example with all parameters",
    "inputs": [
        {
            "url": "https://raw.githubusercontent.com/elixir-cloud-aai/CrateGen/main/README.md",
            "path": "/input/README.md"
        }
    ],
    "outputs": [
